{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "target",
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es2018",
      "esnext"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "~/*": [
        "src/*"
      ],
      "@wooportal/*": [
        "target/@wooportal/*"
      ]
    }
  }
}
