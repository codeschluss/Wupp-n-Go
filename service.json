{
  "index": "/",
  "assetGroups": [
    {
      "name": "client",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/",
          "/manifest.json",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/css/*.css",
          "/i18n/**",
          "/images/**",
          "/videos/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "/api/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout":"5m",
        "maxAge": "1d"
      }
    }
  ]
}
