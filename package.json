{
  "name": "@wooportal/client",
  "version": "0.0.5",
  "description": "client for wooportal - conventional event mapping",
  "homepage": "https://github.com/wooportal/client",
  "author": {
    "name": "codeschluss",
    "email": "info@codeschluss.de",
    "url": "https://www.codeschluss.de"
  },
  "bugs": {
    "email": "info@codeschluss.de",
    "url": "https://github.com/wooportal/client/issues"
  },
  "config": {
    "api": {
      "rootUrl": "/api",
      "authUrl": "/api/login",
      "refreshUrl": "/api/refresh"
    },
    "jwtClaims": {
      "activityProvider": "createdActivities",
      "organisationAdmin": "adminOrgas",
      "organisationUser": "approvedOrgas",
      "superUser": "superuser",
      "userId": "id"
    },
    "position": {
      "city": ""
    },
    "translations": {
      "defaultLocale": "de"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooportal/client"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run start:dev",
    "build": "npm run build:app && npm run build:ssr",
    "build:app": "ng build --base-href / --prod @wooportal/client",
    "build:lib": "ng build @wooportal/core && ng build @wooportal/forms",
    "build:ssr": "ng run @wooportal/client:server && tsc -p express.json",
    "build:xlf": "ng xi18n && sed -i '/context/d' i18n/strings.xliff",
    "start:dev": "ng serve --host 0.0.0.0 --port",
    "start:ssr": "node target/express"
  },
  "dependencies": {
    "@angular/animations": "7.2.15",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/forms": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/material": "7.3.7",
    "@angular/material-moment-adapter": "7.3.7",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/platform-server": "7.2.15",
    "@angular/router": "7.2.15",
    "@angular/service-worker": "7.2.15",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@nguniversal/common": "7.1.1",
    "@nguniversal/express-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "@ngx-pwa/local-storage": "7.4.2",
    "@ngx-translate/i18n-polyfill": "1.0.0",
    "@wooportal/core": "file:libs/core",
    "@wooportal/forms": "file:libs/forms",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.8",
    "express": "4.17.0",
    "hammerjs": "2.0.8",
    "moment": "2.24.0",
    "rxjs": "6.5.2",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.9",
    "@angular-devkit/build-ng-packagr": "0.13.9",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@angular/language-service": "7.2.15",
    "@types/node": "12.0.2",
    "acorn": "6.1.1",
    "codelyzer": "5.0.1",
    "ng-packagr": "4.7.1",
    "ng-swagger-gen": "1.4.2",
    "ts-loader": "6.0.1",
    "ts-node": "8.1.0",
    "tsickle": "0.34.2",
    "tslint": "5.16.0",
    "typescript": "3.2.4"
  }
}
