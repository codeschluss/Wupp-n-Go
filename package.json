{
  "name": "@wooportal/client",
  "version": "0.0.5",
  "description": "client for wooportal - conventional event mapping",
  "homepage": "https://github.com/wooportal/client",
  "author": {
    "name": "codeschluss",
    "email": "info@codeschluss.de",
    "url": "https://www.codeschluss.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooportal/client"
  },
  "bugs": {
    "email": "info@codeschluss.de",
    "url": "https://github.com/wooportal/client/issues"
  },
  "config": {
    "api": {
      "rootUrl": "/api",
      "authUrl": "/api/login",
      "refreshUrl": "/api/refresh"
    },
    "jwtClaims": {
      "activityProvider": "createdActivities",
      "organisationAdmin": "adminOrgas",
      "organisationUser": "approvedOrgas",
      "superUser": "superuser",
      "userId": "id"
    },
    "defaults": {
      "city": "",
      "language": "en"
    }
  },
  "android": {
    "v8Flags": "--expose_gc"
  },
  "nativescript": {
    "id": "de.codeschluss.wooportal.client",
    "tns-android": {
      "version": "5.4.0"
    }
  },
  "main": "native.js",
  "scripts": {
    "ng": "ng",
    "tns": "tns",
    "start": "npm run start:dev",
    "setup": "update-ns-webpack --configs && run-s setup:*",
    "build": "run-s setup build:app build:ssr build:ios build:oid",
    "build:app": "ng build --base-href / --prod @wooportal/client",
    "build:ios": "tns build ios --bundle --env.aot --env.uglify",
    "build:oid": "tns build android --bundle --env.aot --env.uglify",
    "build:lib": "ng build @wooportal/core && ng build @wooportal/forms",
    "build:ssr": "ng run @wooportal/client:server && tsc -p express.json",
    "build:xlf": "ng xi18n && sed -i '/context/d' res/All/i18n/strings.xliff",
    "setup:env": "cp package.json src && cp tsconfig.json tsconfig.tns.json",
    "setup:ios": "BUY_MAC && tns platform add ios ||:&& tns prepare ios",
    "setup:oid": "tns platform add android ||:&& tns prepare android",
    "start:dev": "ng serve --host 0.0.0.0 --port",
    "start:oid": "tns debug android --env.aot",
    "start:ssr": "node target/express",
    "start:tns": "tns preview"
  },
  "dependencies": {
    "@angular/animations": "8.1.2",
    "@angular/cdk": "8.1.1",
    "@angular/common": "8.1.2",
    "@angular/compiler": "8.1.2",
    "@angular/core": "8.1.2",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.1.2",
    "@angular/material": "8.1.1",
    "@angular/material-moment-adapter": "8.1.1",
    "@angular/platform-browser": "8.1.2",
    "@angular/platform-browser-dynamic": "8.1.2",
    "@angular/platform-server": "8.1.2",
    "@angular/router": "8.1.2",
    "@angular/service-worker": "8.1.2",
    "@fortawesome/angular-fontawesome": "0.4.0",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-pwa/local-storage": "8.0.2",
    "@ngx-translate/i18n-polyfill": "1.0.0",
    "@wooportal/core": "file:libs/core",
    "@wooportal/forms": "file:libs/forms",
    "express": "4.17.1",
    "lodash": "4.17.14",
    "moment": "2.24.0",
    "nativescript-ui-sidedrawer": "7.0.0",
    "ngx-openlayers": "1.0.0-next.9",
    "ol": "5.3.3",
    "rxjs": "6.5.2",
    "tns-core-modules": "6.0.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.801.1",
    "@angular-devkit/build-ng-packagr": "0.801.1",
    "@angular/cli": "8.1.1",
    "@angular/compiler-cli": "8.1.2",
    "@angular/http": "8.0.0-beta.10",
    "@angular/language-service": "8.1.2",
    "@nativescript/schematics": "0.6.0",
    "@types/node": "12.6.8",
    "acorn": "6.2.0",
    "classlist.js": "1.1.20150312",
    "codelyzer": "5.1.0",
    "core-js": "3.1.4",
    "hammerjs": "2.0.8",
    "nativescript": "6.0.0",
    "nativescript-angular": "8.0.2",
    "nativescript-dev-sass": "1.7.0",
    "nativescript-dev-typescript": "0.10.0",
    "nativescript-dev-webpack": "1.0.1",
    "nativescript-ng-ripple": "2.0.1",
    "nativescript-theme-core": "2.0.2",
    "ng-packagr": "5.3.0",
    "npm-run-all": "4.1.5",
    "reflect-metadata": "0.1.13",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tsickle": "0.36.0",
    "tslib": "1.10.0",
    "tslint": "5.18.0",
    "typescript": "3.4.5",
    "web-animations-js": "github:angular/web-animations-js#release_pr208"
  }
}
